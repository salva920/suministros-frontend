{
  "version": 2,
  "build": {
    "env": {
      "NODE_VERSION": "18"
    }
  },
  "functions": {
    "ferreteria-frontend/ferreteria-backend/server.js": {
      "memory": 3008,
      "maxDuration": 30
    }
  },
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "ferreteria-frontend/ferreteria-backend/server.js",
      "headers": {
        "Access-Control-Allow-Origin": "https://suministros-frontend.vercel.app"
      }
    },
    {
      "src": "/(.*)",
      "dest": "ferreteria-frontend/build/$1",
      "headers": {
        "Cache-Control": "public, max-age=31536000"
      }
    }
  ]
}
